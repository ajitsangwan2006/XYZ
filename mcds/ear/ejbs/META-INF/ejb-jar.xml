<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN' 'http://java.sun.com/dtd/ejb-jar_2_0.dtd'>
<ejb-jar>
    <enterprise-beans>

        <session>
            <description>mcds for mobile contents</description>
            <display-name>mcds</display-name>
            <ejb-name>MCDSEJB</ejb-name>
            <home>com.gisil.mcds.ejb.MCDSHome</home>
            <remote>com.gisil.mcds.ejb.MCDSRemote</remote>
            <ejb-class>com.gisil.mcds.ejb.MCDSBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
    
</ejb-jar>